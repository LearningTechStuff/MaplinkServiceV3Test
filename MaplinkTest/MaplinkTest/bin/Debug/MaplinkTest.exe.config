<?xml version="1.0"?>
<configuration>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup><system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="AuthenticationSoap" />
                <binding name="AddressFinderSoap" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://services.maplink.com.br/webservices/v3/Authentication/Authentication.asmx"
                binding="basicHttpBinding" bindingConfiguration="AuthenticationSoap"
                contract="MaplinkAuth.AuthenticationSoap" name="AuthenticationSoap" />
            <endpoint address="http://services.maplink.com.br/webservices/v3/AddressFinder/AddressFinder.asmx"
                binding="basicHttpBinding" bindingConfiguration="AddressFinderSoap"
                contract="MaplinkWebService.AddressFinderSoap" name="AddressFinderSoap" />
        </client>
    </system.serviceModel>
</configuration>
